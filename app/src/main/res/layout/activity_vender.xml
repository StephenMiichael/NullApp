<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:baselineAligned="false">

    <ImageView
        android:id="@+id/imageV"
        android:layout_width="134dp"
        android:layout_height="123dp"
        android:layout_marginStart="48dp"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="56dp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_close_clear_cancel" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="201dp"
        android:layout_height="34dp"
        android:layout_marginTop="56dp"
        android:text="Nome do produto"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/etCpf8"
        app:layout_constraintTop_toBottomOf="@+id/imageV" />

    <EditText
        android:id="@+id/etCpf6"
        android:layout_width="345dp"
        android:layout_height="53dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <EditText
        android:id="@+id/etCpf7"
        android:layout_width="345dp"
        android:layout_height="53dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.469"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <EditText
        android:id="@+id/etCpf8"
        android:layout_width="345dp"
        android:layout_height="53dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.469"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="201dp"
        android:layout_height="34dp"
        android:layout_marginTop="21dp"
        android:text="Quantidade"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/etCpf7"
        app:layout_constraintTop_toBottomOf="@+id/etCpf8" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="201dp"
        android:layout_height="34dp"
        android:layout_marginTop="35dp"
        android:text="PreÃ§o Unt"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/etCpf6"
        app:layout_constraintTop_toBottomOf="@+id/etCpf7" />

    <Button
        android:id="@+id/button10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="154dp"
        android:layout_marginLeft="154dp"
        android:layout_marginTop="14dp"
        android:text="Cadastrar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etCpf6" />

    <Button
        android:id="@+id/button11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginLeft="29dp"
        android:layout_marginTop="80dp"
        android:onClick="pegaFoto"
        android:text="Button"
        app:layout_constraintStart_toEndOf="@+id/imageV"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Selecionar Imagem" />

</android.support.constraint.ConstraintLayout>